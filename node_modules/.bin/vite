#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/sineyed/Documents/website-github/nevermore23274.github.io/node_modules/.pnpm/vite@5.4.15_@types+node@22.13.14_lightningcss@1.28.2_stylus@0.59.0_terser@5.37.0/node_modules/vite/bin/node_modules:/home/sineyed/Documents/website-github/nevermore23274.github.io/node_modules/.pnpm/vite@5.4.15_@types+node@22.13.14_lightningcss@1.28.2_stylus@0.59.0_terser@5.37.0/node_modules/vite/node_modules:/home/sineyed/Documents/website-github/nevermore23274.github.io/node_modules/.pnpm/vite@5.4.15_@types+node@22.13.14_lightningcss@1.28.2_stylus@0.59.0_terser@5.37.0/node_modules:/home/sineyed/Documents/website-github/nevermore23274.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/sineyed/Documents/website-github/nevermore23274.github.io/node_modules/.pnpm/vite@5.4.15_@types+node@22.13.14_lightningcss@1.28.2_stylus@0.59.0_terser@5.37.0/node_modules/vite/bin/node_modules:/home/sineyed/Documents/website-github/nevermore23274.github.io/node_modules/.pnpm/vite@5.4.15_@types+node@22.13.14_lightningcss@1.28.2_stylus@0.59.0_terser@5.37.0/node_modules/vite/node_modules:/home/sineyed/Documents/website-github/nevermore23274.github.io/node_modules/.pnpm/vite@5.4.15_@types+node@22.13.14_lightningcss@1.28.2_stylus@0.59.0_terser@5.37.0/node_modules:/home/sineyed/Documents/website-github/nevermore23274.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/vite@5.4.15_@types+node@22.13.14_lightningcss@1.28.2_stylus@0.59.0_terser@5.37.0/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../.pnpm/vite@5.4.15_@types+node@22.13.14_lightningcss@1.28.2_stylus@0.59.0_terser@5.37.0/node_modules/vite/bin/vite.js" "$@"
fi
