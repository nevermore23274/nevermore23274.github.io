#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/sineyed/Documents/website-github/nevermore23274.github.io/node_modules/.pnpm/prettier@2.8.7/node_modules/prettier/node_modules:/home/sineyed/Documents/website-github/nevermore23274.github.io/node_modules/.pnpm/prettier@2.8.7/node_modules:/home/sineyed/Documents/website-github/nevermore23274.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/sineyed/Documents/website-github/nevermore23274.github.io/node_modules/.pnpm/prettier@2.8.7/node_modules/prettier/node_modules:/home/sineyed/Documents/website-github/nevermore23274.github.io/node_modules/.pnpm/prettier@2.8.7/node_modules:/home/sineyed/Documents/website-github/nevermore23274.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin-prettier.js" "$@"
else
  exec node  "$basedir/../../bin-prettier.js" "$@"
fi
